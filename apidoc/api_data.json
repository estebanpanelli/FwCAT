            "allowedValues": [
              "\"objects\"",
              "\"objectgroups\"",
              "\"interfaces\"",
              "\"users\""
            ],
            "optional": false,
            "field": "key",
            "description": "<p>Config property to retrieve</p>"
          },
          {
            "group": "Parameter",
            "type": "String",
